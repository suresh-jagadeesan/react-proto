    "setupFiles": [
      "./test/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suresh-jagadeesan/react-proto.git"
  },
  "keywords": [
    "react",
    "redux",
    "server-side-rendering",
    "prototype"
  ],
  "author": "Sako Hartounian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/suresh-jagadeesan/react-proto/issues"
